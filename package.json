{
  "devDependencies": {
    "@types/eslint": "^8.44.2",
    "@types/node": "^20.5.9",
    "@types/react": "18.2.21",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
